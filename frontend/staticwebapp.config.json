{
  "trailingSlash": "auto",
  "routes": [
    {
      "route": "/",
      "rewrite": "/dist/index.html"
    },
     {
      "route": "/dist/*",
      "rewrite": "/dist/index.html"
    } 
  ],
  "navigationFallback": {
    "rewrite": "/dist/index.html",
    "exclude": [
      "*.{css,scss,js,png,gif,ico,jpg,svg}"
    ]
  }
}
