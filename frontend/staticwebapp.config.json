{
  "trailingSlash": "auto",
  "routes": [
    {
      "route": "/",
      "rewrite": "/frontend/dist/index.html"
    },
    {
      "route": "/index",
      "rewrite": "/frontend/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/dist/index.html",
    "exclude": [
      "*.{css,scss,js,png,gif,ico,jpg,svg}"
    ]
  }
}
